<Window x:Class="xFunc.App.View.LogicView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:res="clr-namespace:xFunc.App.Resources"
        Title="{x:Static res:Resource.logicViewTitle}" Width="800" Height="450" WindowStartupLocation="CenterOwner">
    <Window.Resources>
        <Style TargetType="{x:Type ListViewItem}">
            <Setter Property="HorizontalContentAlignment" Value="Center" />
        </Style>
    </Window.Resources>
    <ListView x:Name="tableList" ItemsSource="{Binding Path=Table}" />
</Window>
